{
  "productName": "TTS Helper",
  "version": "4.0.2",
  "identifier": "com.panku.io",
  "bundle": {
    "active": true,
    "icon": [
      "icons/logo.ico"
    ],
    "targets": "all"
  },
  "build": {
    "beforeDevCommand": "yarn start --port 1420",
    "beforeBuildCommand": "yarn build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist/tts-helper"
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/Uhuh/tts-helper/releases/latest/download/latest.json"
      ],
      "pubkey": "W50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDkwRjA0MjEyQUY5N0U5OTYKUldTVzZaZXZFa0x3a0JkYXBqZXNnZDlOa0M4eGRzbHhUNkUvRWNGWk9iY2FyVzNIOWsvZ0w5TUsK",
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "TTS Helper",
        "width": 1000,
        "height": 800,
        "minHeight": 450,
        "minWidth": 350
      }
    ]
  }
}